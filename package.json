    "serve": "firebase serve",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/style-resources": "^0.1.2",
    "@nuxtjs/toast": "^3.2.1",
    "bulma": "^0.7.5",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "firebase": "^6.3.0",
    "nuxt": "^2.0.0",
    "slugify": "^1.3.4",
    "store": "^2.0.12",
    "vee-validate": "^2.2.13",
    "vue-croppa": "^1.3.8",
    "vue-swal": "^1.0.0",
    "vue-warehouse": "^1.0.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^0.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "precss": "^4.0.0",
    "sass": "^1.19.0",
    "sass-loader": "^7.1.0"
