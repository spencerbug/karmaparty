{
  "name": "karmaparty",
  "version": "1.0.1",
  "description": "Karma Party - Socially Impactful Retail Partiest",
  "author": "Spencer Neilan",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "yarn lint",
    "dev": "cross-env HOST=localhost PORT=3040 nuxt",
    "build": "nuxt build && yarn copy",
    "copy": "cp -R functions/.nuxt/dist/ public/assets",
